   "source": [
    "def fibonacci():\n",
    "    a,b = 1, 1\n",
    "    while True:\n",
    "        yield a\n",
    "        a, b = b, a+b\n",
    "\n",
    "gen_fib = fibonacci() #피보나치 제너레이터 생성\n",
    "\n",
    "for fib_num in gen_fib:\n",
    "    if fib_num > 1000:\n",
    "        break\n",
    "    print(fib_num)"
   ]
